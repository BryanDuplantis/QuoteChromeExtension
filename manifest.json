{
  "manifest_version": 2,

  "name": "Don't Quote Me",
  "description": "This extension shows a random quote when loaded.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
