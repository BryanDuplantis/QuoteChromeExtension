{
  "manifest_version": 2,

  "name": "Don't Quote Me",
  "description": "This extension shows a random quote when loaded.",
  "version": "1.0",
  "chrome_url_overrides": {
    "newtab": "redirect.html"
  },

  "browser_action": {
    "default_icon": "quote.png",
    "default_title": "Don't Quote Me"
  },
  "permissions": [
    "activeTab",
    "http://dontquoteme.bryanduplantis.com/"
  ]
}
